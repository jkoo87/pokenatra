<p>Catch Pokemon!</p>


<form class="containareon" action="/trainers/<%= @trainer.id %>" method="get">
  <select name="pokemon">
    <% @pokemon.each do |pokemon| %>
      <option  value="<%= pokemon.id %>"> <%= pokemon.name %> </option>
    <% end %>
  </select>
  <button type="submit">CATCH</button>
</form>
